{
    "homepage": "http://kappafon.github.io/markdown-preview",
    "name": "markdown-preview",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@types/jest": "24.0.23",
        "@types/marked": "^0.7.1",
        "@types/node": "12.12.8",
        "@types/react": "16.9.11",
        "@types/react-dom": "16.9.4",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.3",
        "marked": "^0.7.0",
        "node-sass": "^4.13.0",
        "prettier": "^1.19.1",
        "react": "^16.12.0",
        "react-dom": "^16.12.0",
        "react-scripts": "3.2.0",
        "typescript": "3.7.2"
    },
    "lint-staged": {
        "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write",
            "git add"
        ]
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "gh-pages": "^2.1.1"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
